{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "bundler",
    "outDir": "./dist/server",
    "rootDir": "./src/Server",
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "lib": ["ES2020"],
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": ["src/Server/**/*"],
  "exclude": ["node_modules", "dist", "src/Interface/**/*"]
}